{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "sampleblock/editable",
  "version": "0.1.0",
  "title": "Sample Block",
  "category": "text",
  "icon": "smiley",
  "description": "サンプルブロック",
  "attributes": {
    "content": {
      "type": "string",
      "source": "text",
      "selector": "h1"
    },
    "content2": {
      "type": "string",
      "source": "text",
      "selector": "p",
      "className": "own-text"
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "editable",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}